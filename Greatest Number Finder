
  import java.util.*; 

   public class main 
   {
     public static void main(String[] args) 
     {
          Scanner sc = new Scanner(System.in);
           
          System.out.print("Enter the first number: ");
          int a = sc.nextInt();
            
          System.out.print("Enter the second number: ");
          int b = sc.nextInt();

           if (a>b) 
           {
           System.out.println("The first number " + a + " is greater than second number "+ b);
           }
           else if(b>a)
           {
            System.out.println("The second number "+b+ " is greater than first number "+a);
           }
          else
           {
              System.out.println("Both number " +a+ " & " +b+ " are equal");
           }

   }
}
